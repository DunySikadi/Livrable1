{{#each data}}
<div class="container-game">
    <div class="container-img">
        <img src={{image}} alt="img">
    </div>
    <div class="t-align p-2">
        <h4>{{title}}</h4>
        <a href="/home/{{id}}">
            <button class="btn btn-primary">
                <i class="fa-regular fa-eye"></i>
                Voir +</button>
        </a>
    </div>
</div>
{{/each}}